{
  "name": "nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:db:seed": "prisma db seed",
    "db:reset": "pnpm prisma:migrate:reset && pnpm prisma:generate",
    "db:start": "docker compose up postgres",
    "types:generate:watch": "ts-node scripts/generate-typings.ts"
  },
  "dependencies": {
    "@adminjs/express": "^6.0.0",
    "@adminjs/passwords": "^4.0.0",
    "@adminjs/prisma": "^5.0.1",
    "@apollo/server": "^4.9.2",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/common": "^10.2.2",
    "@nestjs/core": "^10.2.2",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.2.2",
    "@prisma/client": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "adminjs": "^7.2.1",
    "argon2": "^0.31.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "graphql": "^16.8.0",
    "helmet": "^7.0.0",
    "joi": "^17.10.0",
    "nestjs-i18n": "^10.3.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.16",
    "@nestjs/config": "^3.0.0",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.2",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.4",
    "@types/node": "20.5.7",
    "@types/passport-jwt": "^3.0.9",
    "@types/react": "^18.2.21",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "29.6.4",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.2",
    "prisma": "^5.2.0",
    "react": "^18.2.0",
    "source-map-support": "^0.5.21",
    "styled-components": "^6.0.7",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-morph": "^19.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}